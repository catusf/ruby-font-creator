	function have_tone_number()
	{// does the last character is a number ?
		preg_match('/^([a-zü]+)([1-5])$/', $this->WORD, $match);
		// 		var_dump($this->SKIPPED_TONES);
		if (in_array($match[2], $this->SKIPPED_TONES)==TRUE)
		{// this tone doesn't have any particular notation
			$this->WORD = substr($this->WORD, 0, -1);
			return FALSE;
		} else
		{
			return (bool) !empty($match[2]); // negation_of(is_empty())
		}
	}

	function grab_tone_number()
	{// last number give the diacritic tone
		if ($this->have_tone_number()==TRUE) // number are at the end of the string
		{
			$this->NB = substr($this->WORD, -1);
			$this->WORD = substr($this->WORD, 0, -1);
			return TRUE;
		} else
		{
			return FALSE;
		}
	}

	function pinyin_char($char)
	{
		$pinyin_vowels_pos = array_search($char, $this->PINYIN_VOWELS);
		$pos =  (($this->NB-1)*6)+$pinyin_vowels_pos;
		return $this->TONES[$pos];
	}
	function pinyin_char2()
	{
		$pinyin_vowels_pos = array_search($this->WORD[$i], $this->PINYIN_VOWELS);
		$pos =  (($this->NB-1)*6)+$pinyin_vowels_pos;
		return $this->TONES[$pos];
	}
	function pinyin_rule1($i)
	{	// 1. First, look for an "a" or an "e".
		// If either ('a' or 'e') vowel appears, it takes the tone mark.
		// There are no possible pinyin syllables that contain both an "a" and an "e".
		// 		echo $this->WORD[$i].'+';
		switch($this->WORD[$i])
		{
			case 'a':
			case 'e':
				return TRUE;
				break;
			default:
				return FALSE;
		}
	}
	function apply_pinyin_rule1($i)
	{
		$this->WORD = str_replace($this->WORD[$i], $this->pinyin_char(), $this->WORD);
	}

	function pinyin_rule2($i)
	{	// 2. If there is no "a" or "e", look for an "ou".
		// If "ou" appears, then the "o" takes the tone mark.
		static $o = FALSE, $opos, $u = FALSE;

		switch($this->WORD[$i])
		{
			case 'o':
				$o = TRUE;
				break;
			case 'u':
				$u = TRUE;
				break;
			default:
				return FALSE;
		}
		if ($o==TRUE AND $u==TRUE)
		{
			return TRUE;
		} else
		{
			return FALSE;
		}
	}
	function apply_pinyin_rule2($i)
	{
		$this->WORD = str_replace('o', $this->pinyin_char(), $this->WORD);
		echo '['.$this->WORD[$i].'{PVP:'.$pos.'}, '.$this->TONES[$pos].'] — ';

