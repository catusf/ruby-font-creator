	function numbered_to_pinyin()
	{
		if ($this->grab_tone_number()==TRUE)
		{
			//echo '{'.$this->pinyin_char('a').'}';
			$out = str_replace('a', $this->pinyin_char('a'), $this->WORD);
			if ($out==$this->WORD)
			{// we don't find an 'A'
				$out = str_replace('e', $this->pinyin_char('e'), $this->WORD);
				if ($out==$this->WORD)
				{// we don't find an 'E'
					if (!(substr_count($this->WORD, 'ou')>0 AND substr_count($this->WORD, 'uo')>0)==TRUE)
					{// don't find an 'OU' or 'UO'
						$out = str_replace('ü', $this->pinyin_char('ü'), $this->WORD);
						if ($out==$this->WORD)
						{// don't find Umlaut (ü)
							for ($i=strlen($this->WORD); $i>0; $i--)
							{// look for each character in the word begin by the end
								$last_vowel= $this->WORD[$i];
								if (in_array($last_vowel, $this->PINYIN_VOWELS)==TRUE)
								{// add accent on the last vowel
									return  $this->WORD = str_replace($last_vowel, $this->pinyin_char($last_vowel), $this->WORD);
								}
							}
						} else
						{
							return $this->WORD = $out;
						}
					} else
					{// find an 'OU' or a 'UO'
						return $this->WORD = str_replace('o', $this->pinyin_char('o'), $this->WORD);
					}
				} else
				{// find an 'E'
					return $this->WORD = $out;
				}
			} else
			{// find an 'A'
				// 			echo '['.$out.']';
				return $this->WORD = $out;
			}
		}
	}

	// *  A and e trump all other vowels and always take the tone mark.
	// 		(There are no Mandarin syllables in Hanyu Pinyin that contain both a and e).
	// * In the combination ou, o takes the mark.
	// * In all other cases, the final vowel takes the mark.
	function numbered_to_pinyin2()
	{// convert 'huo3' to 'huǒ'
		$this->grab_tone_number();
		echo $this->WORD.'->'.$this->NB.', ';

		for ($i=0; $i<strlen($this->WORD); $i++)
		{// walk through the word to analyse
			if ($this->pinyin_rule1($i)==TRUE)
			{
				$this->apply_pinyin_rule1($i);
				break;
			} else
			{
				if ($this->pinyin_rule2($i)==FALSE)
				{
					$this->apply_pinyin_rule2($i);
				} else
				{
					$this->apply_pinyin_rule3($i);
				}
			}
		}
	}

	function kana_to_romaji()
	{
		return $this->WORD;
	}

	function yale_to_hangul()
	{
		return $this->WORD;
	}

	function choose_romanization()
	{
		switch ($this->ROMANIZATION)
		{
			case 'pinyin':
				$this->SKIPPED_TONES = array('0', '5');
				$this->numbered_to_pinyin();
				break;
			case 'kana':
				$this->kana_to_romaji();
				break;
			case 'yale':
				$this->yale_to_hangul();
				break;
		}
		$this->WORD = strtolower($this->WORD);
	}

	function __construct($str, $romanization)
	{
		$this->ROMANIZATION = $romanization;

		$a_words = explode(' ', mb_strtolower(trim($str), 'UTF-8'));
		// 		var_dump($a_words);
		foreach ($a_words as $this->WORD)
		{
			// 			$this->WORD = $word;
			$this->choose_romanization();
			$this->ROMAN[] = $this->WORD;
		}
		$this->ROMAN = trim(implode(' ', $this->ROMAN));
	}
